class Solution {
    public int findTheWinner(int n, int k) {
        Queue<Integer>q=new LinkedList<>();
        for(int i=1;i<=n;i++)
        {
            q.offer(i);
        }
        while(q.size()>1)
        {
            for(int i=1;i<k;i++)
                q.offer(q.poll());
            q.poll();
        }
        return q.poll();
    }
}

______________________________________________________________________
//less run time 

class Solution {
    public int findTheWinner(int n, int k) {
        int winner=0;
        for(int i=1;i<=n;i++)
           winner= (winner+k)%i;
        return winner+1;
    }
}
