class Solution {
    public Queue<Integer> reverseFirstK(Queue<Integer> q, int k) {
        Stack<Integer> st=new Stack<>();
        if(q==null || k<=0 || k>q.size())
            return q;
        List<Integer> res=new ArrayList<>();
        for(int i=0;i<k;i++)
            res.add(q.poll());
        for(int i=k-1;i>=0;i--)
            q.offer(res.get(i));
        int size = q.size();
        for(int i=0;i<size-k;i++)
            q.offer(q.poll());
        
        return q;
    }
}
